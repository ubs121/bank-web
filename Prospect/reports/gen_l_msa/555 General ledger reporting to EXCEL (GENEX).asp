  function ShowWrkBkList(filename,PrefsFile)
  {
   var delim=",";
   var dropdown,thislist,listlength;
  
   dropdown=document.all("C3,0,0,Worksheet Name");
   
   while (RSAspProxyApplet.readyState != 4) {}
   ASPpage = RSGetASPObject("/prospect/asp/DllCalls.asp");
